<?xml version="1.0" encoding="utf-8"?>
<preset name="jni-linux-aarch64" comment="Linux-aarch64 clang PhysX JNI wrapper general settings">
  <platform targetPlatform="jni-linuxAarch64" compiler="gcc" />
  <CMakeSwitches>
    <cmakeSwitch name="PX_BUILDSNIPPETS" value="False" comment="Generate the snippets" />
    <cmakeSwitch name="PX_BUILDPVDRUNTIME" value="True" comment="Generate the OmniPVD project" />
    <cmakeSwitch name="PX_GENERATE_STATIC_LIBRARIES" value="True" comment="Generate static libs" />
  </CMakeSwitches>
  <CMakeParams>
    <cmakeParam name="CMAKE_INSTALL_PREFIX" value="install/jni-linux-aarch64/PhysX" comment="Install path relative to PhysX SDK root" />
  </CMakeParams>
</preset>